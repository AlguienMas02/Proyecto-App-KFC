<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/tvDatosPersonales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Datos Personales"
        android:textColor="@color/black"
        android:textSize="24sp"
        android:textStyle="bold"
        android:layout_marginTop="64dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>


    <TextView
        android:id="@+id/tvNombreApellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="normal"
        android:textColor="@color/black"
        android:text="Nombre y Apellido"
        android:textSize="18sp"
        android:layout_marginTop="52dp"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toBottomOf="@id/tvDatosPersonales"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="normal"
        android:text="Nombre Apellido"
        android:textColor="@color/black_olive"
        android:textSize="16dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvNombreApellido"
        app:layout_constraintStart_toStartOf="parent"/>

    <LinearLayout
        android:layout_width="315dp"
        android:layout_height="0.5dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="@id/tvNombreApellido"
        app:layout_constraintTop_toBottomOf="@id/tvNombreApellido"/>


    <TextView
        android:id="@+id/tvCelular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:text="NÃºmero de celular"
        android:textSize="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="78dp"
        app:layout_constraintTop_toBottomOf="@id/tvNombreApellido"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="normal"
        android:text="999 888 777"
        android:textColor="@color/black_olive"
        android:textSize="16dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvCelular"
        app:layout_constraintStart_toStartOf="parent"/>

    <LinearLayout
        android:layout_width="315dp"
        android:layout_height="0.5dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="@id/tvCelular"
        app:layout_constraintTop_toBottomOf="@id/tvCelular"/>

    <TextView
        android:id="@+id/tvDocumento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:text="Documento de Identidad"
        android:textSize="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="78dp"
        app:layout_constraintTop_toBottomOf="@id/tvCelular"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="normal"
        android:text="DNI: 87654321"
        android:textColor="@color/black_olive"
        android:textSize="16dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvDocumento"
        app:layout_constraintStart_toStartOf="parent"/>

    <LinearLayout
        android:layout_width="315dp"
        android:layout_height="0.5dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="@id/tvDocumento"
        app:layout_constraintTop_toBottomOf="@id/tvDocumento"/>

    <TextView
        android:id="@+id/tvFechaNacimiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/black"
        android:textStyle="normal"
        android:text="Fecha de Nacimiento"
        android:textSize="18dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="78dp"
        app:layout_constraintTop_toBottomOf="@id/tvDocumento"
        app:layout_constraintStart_toStartOf="parent"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="normal"
        android:text="01/01/2025"
        android:textColor="@color/black_olive"
        android:textSize="16dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@id/tvFechaNacimiento"
        app:layout_constraintStart_toStartOf="parent"/>
    <LinearLayout
        android:layout_width="315dp"
        android:layout_height="0.5dp"
        android:layout_marginTop="48dp"
        app:layout_constraintStart_toStartOf="@id/tvFechaNacimiento"
        app:layout_constraintTop_toBottomOf="@id/tvFechaNacimiento"/>

</androidx.constraintlayout.widget.ConstraintLayout>